{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [{
      "name": "next"
    }],
    "paths": {
      "@/*": ["./src/*"],
      "pro-ui": ["../../packages/pro-ui/src"],
      "pro-ui/*": ["../../packages/pro-ui/src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "../../config/style.constants.js"],
  "exclude": ["node_modules"]
}